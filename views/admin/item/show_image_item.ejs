<div class="tab-pane fade show active" id="show_image" role="tabpanel" aria-labelledby="home-tab">
    <div class="row">
        <% for(let i = 0; i < item.image_id.length; i++) { %>
            <div class="col-md-4">
                <div class="card shadow mb-4 mt-4">
                    <div class="card-body">
                        <img src="<%= base_url + '/' + item.image_id[i].image %>" alt="<%= item.name %>" style="width: 100%; display: block;" />
                    </div>
                </div>
            </div>
        <% } %>
    </div>
    <a href="/admin/item" class="btn btn-info">Kembali</a>
  </div>
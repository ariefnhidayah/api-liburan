<div class="card shadow mb-4">
    <div class="card-header py-3">
      <h6 class="m-0 font-weight-bold text-primary">DataTables Example</h6>
    </div>
    <div class="card-body">
      <div class="table-responsive">
        <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
          <thead>
            <tr>
                <th>No.</th>
                <th>Invoice</th>
                <th>Tanggal</th>
                <th>Nama Item</th>
                <th>Nama Member</th>
                <th>Nama Bank</th>
                <th>Status</th>
                <th>Opsi</th>
            </tr>
          </thead>
          <tbody>
            <% for(let i = 0; i < bookings.length; i++) { %>
              <tr>
                <td><%= i + 1 %></td>
                <td>#<%= bookings[i].invoice %></td>
                <td>
                  <%= bookings[i].date_start.toLocaleString().split(',')[0] %> s/d <%= bookings[i].date_end.toLocaleString().split(',')[0] %>
                </td>
                <td><%= bookings[i].item_id.title %></td>
                <td><%= bookings[i].member_id.first_name %> <%= bookings[i].member_id.last_name %></td>
                <td><%= bookings[i].payments.bank_from %></td>
                <td><%= bookings[i].payments.status %></td>
                <td>
                  <a href="/admin/booking/<%= bookings[i]._id %>" type="button" class="btn btn-primary btn-sm btn-edit"><i class="fas fa-eye"></i></a>
                </td>
              </tr>
            <% } %>
          </tbody>
        </table>
      </div>
    </div>
</div>